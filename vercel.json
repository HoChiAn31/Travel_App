{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/user/(.*)",
            "dest": "/user/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}
